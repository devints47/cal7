{
  "name": "cal7",
  "version": "1.0.0",
  "description": "A zero-configuration React calendar component for Google Calendar events",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./route": {
      "import": "./dist/route.esm.js",
      "require": "./dist/route.js",
      "types": "./dist/route.d.ts"
    },
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "dev": "rollup -c --watch",
    "type-check": "tsc --noEmit",
    "lint": "echo 'Linting will be configured in a later task'",
    "lint:fix": "echo 'Linting will be configured in a later task'",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build",
    "size": "rollup -c --bundleAnalyzer"
  },
  "keywords": [
    "react",
    "calendar",
    "google-calendar",
    "events",
    "component",
    "typescript",
    "nextjs"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/devints47/cal7.git"
  },
  "bugs": {
    "url": "https://github.com/devints47/cal7/issues"
  },
  "homepage": "https://github.com/devints47/cal7#readme",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "next": ">=13.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/react": "^18.3.17",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.18.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "postcss": "^8.5.1",
    "rollup": "^4.28.1",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "@testing-library/react": "^16.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "jsdom": "^25.0.1",
    "tslib": "^2.8.1"
  },
  "dependencies": {
    "dompurify": "^3.2.3",
    "focus-trap": "^7.6.2",
    "zod": "^3.24.1"
  }
}